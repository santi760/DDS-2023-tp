<br><br><br>
{{#partial "contenido"}}
    <div>
        <h2>Establecimientos Cargados para {{entity.name}}</h2>
    </div>
    <div class="table-responsive">
        <table class="table table-striped">
            <thead>
            <tr>
                <th>Nombre</th>
            </tr>
            </thead>
            <tbody>
            {{#each establishments}}
                <tr>
                    <td>{{ this.name }}</td>
                    <td>{{ this.description }}</td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    </div>
    <div style="text-align: center;">
        <a class="btn btn-primary" href="/entity/{{entity.id}}/establishments">Volver</a>
    </div>
    <br>

{{/partial}}
{{> templates/components/layout/LayoutBaseProvider}}